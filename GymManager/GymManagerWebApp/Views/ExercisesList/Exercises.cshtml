
@model Exercises


@{
    <div>
                <div class="row-flex-space">
                    <table>
                        <tr>
                            <th>L.P.</th>
                            <th>Nazwa zajęć</th>
                            <th>Data zajęć</th>
                            <th>Czas trwania</th>
                        </tr>
                        @{ foreach (var value in Model.GetAllExercises())
                            {
                                <tr>
                                    <td>
                                        @value.Id
                                    </td>
                                    <td>
                                        @value.ExerciseName
                                    </td>
                                      <td>
                                        @value.ExerciseDate
                                    </td>
                                     <td>
                                        @value.ExerciseDurationTime
                                    </td>
                                </tr>
                            }
                        }
                    </table>
                </div>
    </div>
}
